{
    "version": "1.0",
    "projections": {
        "pokemon-service-server-sdk": {
            "imports": [
                "./pokemon.smithy"
            ],
            "plugins": {
                "rust-server-codegen": {
                    "service": "aws.dev#PokemonService",
                    "module": "pokemon-service-server-sdk",
                    "moduleDescription": "Rust server SDK for the Pokémon Service",
                    "moduleVersion": "0.1.0",
                    "moduleAuthors": [
                        "Tim McNamara <timmcn@amazon.com>",
                        "Bigo <bigo@crisidev.org>"
                    ],
                    "runtimeConfig": {
                        "version": "DEFAULT"
                    }
                }
            }
        },
        "pokemon-service-client-sdk": {
            "imports": [
                "./pokemon.smithy"
            ],
            "plugins": {
                "rust-codegen": {
                    "service": "aws.dev#PokemonService",
                    "module": "pokemon-service-client-sdk",
                    "moduleDescription": "Rust client SDK for the Pokémon Service",
                    "moduleVersion": "0.1.0",
                    "moduleAuthors": [
                        "Tim McNamara <timmcn@amazon.com>",
                        "Bigo <bigo@crisidev.org>"
                    ],
                    "runtimeConfig": {
                        "version": "DEFAULT"
                    }
                }
            }
        }
    }
}

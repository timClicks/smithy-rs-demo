{
    "version": "1.0",
    "projections": {
        "solar-system-catalog-server-sdk": {
            "imports": [
                "./solarsystem.smithy"
            ],
            "plugins": {
                "rust-server-codegen": {
                    "service": "mcnamara.nz#SolarSystemCatalog",
                    "module": "solar-system-catalog-server-sdk",
                    "moduleDescription": "Rust-based server SDK for the catalog.",
                    "moduleVersion": "0.1.0",
                    "moduleAuthors": [
                        "Tim McNamara <timmcn@amazon.com>",
                        "Bigo <bigo@crisidev.org>"
                    ],
                    "runtimeConfig": {
                        "version": "DEFAULT"
                    }
                }
            }
        },
        "solar-system-catalog-client-sdk": {
            "imports": [
                "./solarsystem.smithy"
            ],
            "plugins": {
                "rust-codegen": {
                    "service": "mcnamara.nz#SolarSystemCatalog",
                    "module": "solar-system-catalog-client-sdk",
                    "moduleDescription": "Rust-based client SDK for the catalog.",
                    "moduleVersion": "0.1.0",
                    "moduleAuthors": [
                        "Tim McNamara <timmcn@amazon.com>",
                        "Bigo <bigo@crisidev.org>"
                    ],
                    "runtimeConfig": {
                        "version": "DEFAULT"
                    }
                }
            }
        }
    }
}
